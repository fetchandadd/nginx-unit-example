{
	"listeners": {
		"*:8081": {
			"application": "helloPython"
		},

		"*:8082": {
			"application": "helloPhp"
		}
	},

	"applications": {
		"helloPython": {
			"type": "python",
			"processes": 2,
			"path": "/var/www/python",
			"module": "wsgi"
		},

		"helloPhp": {
			"type": "php",
			"processes": 2,
			"root": "/var/www/php",
			"index": "index.php"
		}
	}
}
